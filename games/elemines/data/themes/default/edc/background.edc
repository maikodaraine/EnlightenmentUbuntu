/* background */
group {
  name: "bg";
  parts {
    part {
      name: "bg";
      type: IMAGE;
      description {
        state: "default" 0.0;
        image.normal: "sky.png";
        fill {
          size {
            relative: 0.0 0.0;
            offset: 150 180;
          }
        }
      }
    }
  }
}
